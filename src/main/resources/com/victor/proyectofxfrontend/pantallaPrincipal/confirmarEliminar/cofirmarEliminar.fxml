<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.victor.proyectofxfrontend.vista.pantallaPrincipal.centroDinamico.perfil.confirmarEliminar.ConfirmarEliminarController"
            prefHeight="300" prefWidth="300" stylesheets="@estiloConfirmarEliminar.css">
    <VBox spacing="10" alignment="CENTER" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.bottomAnchor="10">
        <Label text="Introducir tu contraseña para confirmar:"/>
        <PasswordField fx:id="passwordField" promptText="Contraseña"/>
        <HBox spacing="10" alignment="CENTER">
            <Button text="Cancelar" onAction="#cancelar"/>
            <Button text="Eliminar cuenta" onAction="#confirmarEliminar" styleClass="confirmar"/>
        </HBox>
    </VBox>
</AnchorPane>
